import TestePratico1.Singleton.*;

public class Main {
    public static void main(String[] args) {
        // Obter a instância única do Logger
        Logger logger = Logger.getInstance();
        Logger logger1 = Logger.getInstance();

        // Utilizar o método log para imprimir mensagens
        logger.log("Início do programa");
        logger1.log("Processando dados...");
        logger.log("Fim do programa");

        System.out.println(logger1 == logger);
    }
}